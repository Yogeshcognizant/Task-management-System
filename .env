# Azure OpenAI Configuration (Use your personal Azure account)
AZURE_OPENAI_KEY=your_azure_openai_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/

# Microsoft Graph API (Optional - for calendar integration)
GRAPH_ACCESS_TOKEN=your_graph_access_token_here

# Teams Bot (Only needed for Teams integration)
MICROSOFT_APP_ID=your_bot_app_id_here
MICROSOFT_APP_PASSWORD=your_bot_app_password_here
AZURE_TENANT_ID=your_tenant_id_here